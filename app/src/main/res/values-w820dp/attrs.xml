<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>


    <declare-styleable name="DogTelTextView">
        <attr name="font_style" format="enum">
            <enum name="regular" value="0" />
            <enum name="light" value="1" />
            <enum name="heavy" value="2" />
            <enum name="italic" value="3" />
            <enum name="bold" value="4" />
            <enum name="bold_italic" value="5" />
            <enum name="heavy_italic" value="6" />
            <enum name="light_italic" value="7" />
        </attr>
    </declare-styleable>
</resources>